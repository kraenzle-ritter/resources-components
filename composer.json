{
  "name": "kraenzle-ritter/resources-components",
  "description": "Livewire components for resources",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Kraenzle",
      "email": "kraenzle@k-r.ch",
      "homepage": "https://www.k-r.ch"
    }
  ],
  "homepage": "https://github.com/kraenzle-ritter/resources-components",
  "keywords": [
    "Laravel",
    "resources-components",
    "livewire-component",
    "GND"
  ],
  "require": {
    "laravel/framework": ">=10",
    "guzzlehttp/guzzle": "^7.8",
    "livewire/livewire": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9",
    "mockery/mockery": "^1.1",
    "orchestra/testbench": "^9",
    "sempro/phpunit-pretty-print": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "KraenzleRitter\\ResourcesComponents\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KraenzleRitter\\ResourcesComponents\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "KraenzleRitter\\ResourcesComponents\\ResourcesComponentsServiceProvider"
      ]
    }
  }
}
